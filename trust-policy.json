{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "Lambda",
        "Effect": "Allow",
        "Principal": {
          "Service": "lambda.amazonaws.com"
        },
        "Action": "sts:AssumeRole"
      },
      {
        "Sid": "DynamoDB",
        "Effect": "Allow",
        "Principal": {
          "Service": "dynamodb.amazonaws.com"
        },
        "Action": "sts:AssumeRole"
      }
    ]
}
